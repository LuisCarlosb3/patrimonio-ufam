{
  "name": "procap-api",
  "version": "1.0.0",
  "description": "API Rest para o gerenciamento de patrim√¥nio do ICET",
  "main": "dist/main/server.js",
  "scripts": {
    "start": "node dist/main/server.js",
    "start-dev": "ts-node src/main/server.ts",
    "buld": "rimraf dist && tsc -p tsconfig-build.json",
    "test": "jest --passWithNoTests --silent --noStackTrace --detectOpenHandles --runInBand --no-cache",
    "test:verbose": "jest --passWithNoTests --runInBand",
    "test:unit": "npm test -- --watch -c jest-unit-config.js",
    "test:integration": "npm test -- --watch -c jest-integration-config.js",
    "test:staged": "npm test --findRelatedTests",
    "test:ci": "npm test --coverage",
    "knex:migrate:make":"npx knex --knexfile src/main/config/knexfile.ts migrate:make -x ts",
    "knex:migrate:latest": "npx knex --knexfile src/main/config/knexfile.ts migrate:latest",
    "knex:migrate:down": "npx knex --knexfile src/main/config/knexfile.ts migrate:down"
  },
  "repository": {
    "type": "git",
    "url": "git+https://gitlab.com/lu.ca.oliveira41/procap.git"
  },
  "author": "Area 01",
  "license": "ISC",
  "bugs": {
    "url": "https://gitlab.com/lu.ca.oliveira41/procap/issues"
  },
  "homepage": "https://gitlab.com/lu.ca.oliveira41/procap#readme",
  "devDependencies": {
    "@types/bcrypt": "^3.0.1",
    "@types/express": "^4.17.11",
    "@types/jest": "^26.0.23",
    "@types/jsonwebtoken": "^8.5.1",
    "@types/knex": "^0.16.1",
    "@types/mongodb": "^3.6.12",
    "@types/node-cron": "^2.0.3",
    "@types/nodemailer": "^6.4.1",
    "@types/pg": "^7.14.11",
    "@types/pino": "^6.3.8",
    "@types/supertest": "^2.0.11",
    "@types/validator": "^13.1.3",
    "@typescript-eslint/eslint-plugin": "^3.10.1",
    "@typescript-eslint/parser": "^3.10.1",
    "eslint": "^7.25.0",
    "eslint-config-standard": "^14.1.1",
    "eslint-config-standard-with-typescript": "^18.0.2",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^4.3.1",
    "eslint-plugin-standard": "^4.1.0",
    "husky": "^4.3.8",
    "jest": "^26.6.3",
    "lint-staged": "^10.5.4",
    "mockdate": "^3.0.5",
    "rimraf": "^3.0.2",
    "supertest": "^4.0.2",
    "ts-jest": "^26.5.6",
    "ts-node": "^8.10.2",
    "typescript": "^3.9.9"
  },
  "dependencies": {
    "bcrypt": "^5.0.1",
    "express": "^4.17.1",
    "jsonwebtoken": "^8.5.1",
    "knex": "^0.95.4",
    "module-alias": "^2.2.2",
    "mongodb": "^3.6.6",
    "node-cron": "^2.0.3",
    "nodemailer": "^6.6.0",
    "pg": "^8.6.0",
    "pino": "^6.11.3",
    "pino-pretty": "^4.7.1",
    "validator": "^13.6.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "pre-push": "npm run test:ci"
    }
  },
  "_moduleAliases": {
    "@": "dist"
  }
}
